<!DOCTYPE html>
<html>
    <head>
        @Html.Partial("_partials/_head")
        @Html.Partial("_partials/_contactHead")
        @RenderSection("head", required: false)
        <link href="//cdn.muicss.com/mui-0.10.3/css/mui.min.css" rel="stylesheet" type="text/css" />
    </head>
    <body class="main">
        @Html.Partial("_partials/_header")
        <div id="site_container" class="container-fluid flex-fill">
            <div class="row">
                <main id="page_container" role="main" class="col">
                    <section class="body-section">
                        <div class="body-container">
                                    @RenderBody()
                        </div>
                    </section>
                </main>
            </div>
        </div>
    </body>
        <footer id="footer">
        @Html.Partial("_partials/_footer")
    </footer>
</html>